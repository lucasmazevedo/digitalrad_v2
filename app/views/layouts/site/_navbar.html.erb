<header>
  <div id="fixedheader" class="header" style="top: 0px;">
    <div class="header-area">
      <div class="logo">
        <%= link_to root_path, title: "Digitalrad - Número 1 em Teleradiologia", data: { turbolinks: false } do %>
        <%= image_tag("logo.png", width: "200", alt: "Digitalrad - Número 1 em Teleradiologia", style: "transform: scale(1);") %>
      <% end %>
      </div>
      <!-- /logo -->
      <div class="header-rightpart">
        <div class="primarymenu menuwrap">
          <ul class="sf-menu">
            <li class="current-menu-ancestor">
              <%= link_to "Página Inicial <i class='fa fa-chevron-down'></i>".html_safe, root_path, data: { turbolinks: false }%>
            <ul>
              <li><%= link_to "Como Funciona", root_path(anchor: "como-funciona"), data: { turbolinks: false }%></li>
              <li><%= link_to "Vantagens Para seu Serviço", root_path(anchor: "nosso-diferencial"), data: { turbolinks: false }%></li>
              <li><%= link_to "Solução Completa em Laudos à Distancia", root_path(anchor: "nossas-solucoes"), data: { turbolinks: false }%></li>
            </ul>
            </li>

            <li><%= link_to "Quem Somos <i class='fa fa-chevron-down'></i>".html_safe, quem_somos_path, data: { turbolinks: false }%>
            <ul>
              <li><%= link_to "A Digitalrad", quem_somos_path(anchor: "a-digitalrad"), data: { turbolinks: false }%></li>
