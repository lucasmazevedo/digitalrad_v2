            <ul>
              <% Report.all.each do |report| %>
                <li><%= link_to report.title, laudos_distancia_path(anchor: "#{report.slug}"), data: { turbolinks: false }%></li>
              <% end %>
            </ul>
            </li>
            <li><%= link_to "Casos Clínicos".html_safe, casos_clinicos_path, data: { turbolinks: false }%></li>
            <li><%= link_to "<i class='fa fa-lock' style='color:red;''></i>  Contratos".html_safe, contratos_path, data: { turbolinks: false }%></li>
